<div class="container-fluid" id="importTransactionsContainer">
	<div class="wrapper wrapper-p-2">
		<h1 class="title-medium">استيراد المعاملات من ملف Excel</h1>
	</div>
	<div class="inner-wrapper wrapper-p-2 mt-4">
		<form action="#" id="addForm">
			<div class="form-group mb-2">
				<label for="" class="form-label">تنسيق الأعمدة</label>
				<ul class="list-group list-group-flush">
					<li class="list-group-item">customerName</li>
					<li class="list-group-item">customerCCP</li>
					<li class="list-group-item">customerMonthlyDeductionAmount</li>
					<li class="list-group-item">customerDateBirth</li>
					<li class="list-group-item">providerPhone</li>
				</ul>
			</div>
			<div class="form-group mb-2">
				<label for="" class="form-label">فتح ملف CSV</label>
				<input type="file" class="input-text" id="csvFileInput">
			</div>
			<div class="form-group">
				<input type="submit" class="btn btn-primary btn-sm" value="اظافة المعاملات">
			</div>
		</form>
	</div>
	<div class="inner-wrapper wrapper-p-2 mt-4">
		<div id="pagination" class="flex flex-center my-3"></div>
		<div class="table-nav">
			<button class="btn btn-danger btn-sm" id="deleteSelectedBTN">حذف</button>
			<div class="form-group mt-2">
				<input type="text" class="input-text input-text-outline" id="searchInput" placeholder="بحث...">
			</div>
		</div>
		<div class="table-div">
			<table class="table table-stripped table-hover" id="tableElement">
				<thead>
					<th id="selectAllBTN">تحديد الكل</th>
					<th>اسم</th>
					<th>حساب CCP</th>
					<th>تاريخ الازدياد</th>
					<th>مبلغ الاقتطاع الشهري</th>
					<th>عنوان الممون</th>
					<th>تمت المعاملة في</th>
				</thead>
				<tbody>
					<!--
					<tr>
						<td>
							<input type="checkbox" class="form-check-input" data-role="CHECK" data-customerid>
						</td>
						<td>name</td>
						<td>ccp</td>
						<td>date_birth</td>
						<td>mothly_ammount</td>
						<td>date</td>
					</tr>
					-->
				</tbody>
			</table>
		</div>
	</div>
</div>